<?xml version="1.0" encoding="utf-8"?>
<package >
  <metadata>
    <id>ApproovHttpClient-Platform-Specific</id>
    <version>3.0.0</version>
    <title>Approov</title>
    <authors>ivol</authors>
    <owners>Criticalblue</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">MIT</license>
    <projectUrl>http://approov.io/</projectUrl>
    <icon>approov.png</icon>
    <description>Custom HttpClient implementation using Approov API protection</description>
    <releaseNotes>Android uses JSON getPins native call</releaseNotes>
    <copyright>Copyright Criticalblue 2020</copyright>
    <tags>API Protection Attestation Mobile</tags>
    <dependencies>
        <group targetFramework="MonoAndroid10">
            <dependency id="ApproovHttpClient" version="1.0.7" />
            <dependency id="ApproovSDK" version="2.7.0.0" />
            <dependency id="Newtonsoft.Json" version="13.0.1" />
            <dependency id="GoogleGson" version="2.8.8" />
            <dependency id="Xamarin.AndroidX.AppCompat" version="1.2.0.5" />
        </group>
        <group targetFramework="Xamarin.iOS10">
            <dependency id="ApproovHttpClient" version="1.0.13" />
            <dependency id="ApproovSDK" version="3.0.0" />
            <dependency id="Newtonsoft.Json" version="13.0.1" />
        </group>
    </dependencies>
  </metadata>
  <!-- Insert below <metadata> element -->
<files>
    <!-- Cross-platform reference assemblies -->
        <file src="approov.png" target="" />
    <!-- iOS reference assemblies -->
        <file src="ApproovHttpClientiOS\bin\Release\ApproovHttpClient.iOS.dll" target="lib/Xamarin.iOS10/ApproovHttpClient.iOS.dll" />
    <!-- Android reference assemblies -->
        <file src="ApproovHttpClientAndroid\bin\Release\ApproovHttpClient.Android.dll" target="lib/MonoAndroid10/ApproovHttpClient.Android.dll" />
</files>

</package>
